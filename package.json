{
  "name": "everything-cli",
  "version": "1.0.0",
  "description": "A CLI for building software.",
  "main": "outputs/everythingCLI.js",
  "scripts": {
    "build": "tsc && node build.cjs",
    "clean": "rimraf coverage outputs",
    "test": "npm run build && c8 node -r source-map-support/register outputs/tests/tests.js",
    "server": "npm run build && node outputs/everythingServer/everythingServer.js"
  },
  "devDependencies": {
    "@types/node": "24.8.1",
    "c8": "10.1.3",
    "esbuild": "0.25.11",
    "rimraf": "6.0.1",
    "source-map-support": "0.5.21",
    "typescript": "5.9.3"
  }
}
